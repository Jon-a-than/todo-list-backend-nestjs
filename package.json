{
  "name": "todo-list",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT",
  "type": "commonjs",
  "scripts": {
    "build": "nest build",
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "format": "biome format . --write",
    "generate": "prisma generate",
    "lint": "eslint \"{src,apps,e2e,test}/**/*.ts\" --fix",
    "prepare": "simple-git-hooks",
    "start": "node dist/main",
    "test": "vitest run -c config/vitest.config.mts",
    "test:watch": "vitest -c config/vitest.config.mts",
    "test:cov": "vitest run --coverage -c config/vitest.config.mts",
    "test:e2e": "vitest run -c config/vitest.e2e.mts"
  },
  "dependencies": {
    "@nestjs/common": "^10.2.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.2.10",
    "@nestjs/mapped-types": "^2.0.4",
    "@nestjs/platform-express": "^10.2.10",
    "@prisma/client": "^5.7.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "prisma": "^5.7.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.4.1",
    "@nestjs/cli": "^10.2.1",
    "@nestjs/schematics": "^10.0.3",
    "@nestjs/testing": "^10.2.10",
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.100",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@vitest/coverage-v8": "^1.0.2",
    "chalk": "^4.0.0",
    "eslint": "^8.55.0",
    "lint-staged": "^15.2.0",
    "simple-git-hooks": "^2.9.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "unplugin-swc": "^1.4.4",
    "vitest": "^1.0.2"
  },
  "lint-staged": {
    "*.{vue,js,jsx,cjs,mjs,ts,tsx,cts,mts}": "eslint --cache --fix",
    "*.{js,jsx,cjs,mjs,ts,tsx,cts,mts}": "biome format --write"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged",
    "pre-push": "pnpm test"
  }
}
